<MudBreadcrumbs Items="Items" Separator=">"></MudBreadcrumbs>

@code {
    private List<BreadcrumbItem> _items = new List<BreadcrumbItem>
{
new BreadcrumbItem("Home", "/")
};
    [Parameter]
    public List<BreadcrumbItem> Items
    {
        get => _items; 
        set
        {
            _items = new List<BreadcrumbItem> { new BreadcrumbItem("Home", "/",icon: Icons.Material.Filled.Home) };
            if (value != null)
            {
                _items.AddRange(value);
            }
        }
    }
}